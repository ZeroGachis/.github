{
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "release-type": "terraform-module",
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "feature",
          "section": "feature"
        },
        {
          "type": "fix",
          "section": "Bug Fixes"
        },
        {
          "type": "perf",
          "section": "Performance Improvements"
        },
        {
          "type": "revert",
          "section": "Reverts"
        },
        {
          "type": "docs",
          "section": "Documentation"
        },
        {
          "type": "chore",
          "section": "Miscellaneous Chores"
        },
        {
          "type": "refactor",
          "section": "Code Refactoring"
        },
        {
          "type": "test",
          "section": "Tests"
        },
        {
          "type": "build",
          "section": "Build System"
        },
        {
          "type": "ci",
          "section": "Continuous Integration"
        }
      ]
    }
  },
  "extra-files": [
    {
      "type": "yaml",
      "path": ".github/workflows/build-xamarin-lib.yml",
      "jsonpath": "jobs.build.steps[1].uses"
    },
    {
      "type": "yaml",
      "path": ".github/workflows/build-xamarin-lib.yml",
      "jsonpath": "jobs.build.steps[2].uses"
    },
    {
      "type": "yaml",
      "path": ".github/workflows/build-xamarin-lib.yml",
      "jsonpath": "jobs.build.steps[3].uses"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
